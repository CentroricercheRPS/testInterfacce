{
    "interface_name": "com.rps.DatalogExternalSensors",
    "version_major": 0,
    "version_minor": 1,
    "type": "datastream",
    "ownership": "device",
    "aggregation": "object",
    "description": "Valori da storicizzare dei sensori ambientali. Il pacchetto dati viene inviato dalla Netman con una frequenza fissa di 60 minuti. I dati devono essere archiviati per 1/5 anni",
    "mappings": [
        {
            "endpoint": "/dataloges/t1",
            "type": "integer",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 157680000,
            "explicit_timestamp": true,
            "description": "Sensor 1 temperature [°C]"
        },
        {
            "endpoint": "/dataloges/t2",
            "type": "integer",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 157680000,
            "explicit_timestamp": true,
            "description": "Sensor 2 temperature [°C]"
        },
        {
            "endpoint": "/dataloges/t3",
            "type": "integer",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 157680000,
            "explicit_timestamp": true,
            "description": "Sensor 3 temperature [°C]"
        },
        {
            "endpoint": "/dataloges/h1",
            "type": "integer",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 157680000,
            "explicit_timestamp": true,
            "description": "Sensor 1 humidity [%RH]"
        },
        {
            "endpoint": "/dataloges/h2",
            "type": "integer",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 157680000,
            "explicit_timestamp": true,
            "description": "Sensor 2 humidity [%RH]"
        },
        {
            "endpoint": "/dataloges/h3",
            "type": "integer",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 157680000,
            "explicit_timestamp": true,
            "description": "Sensor 3 humidity [%RH]"
        }
    ]
}
